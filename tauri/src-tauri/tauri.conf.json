{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tsw_io",
  "version": "0.1.3",
  "identifier": "dev.restifo.tws-io",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null
    },
    "windows": []
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [
      "binaries/tsw_io_backend",
      "binaries/avrdude"
    ],
    "resources": [
      "binaries/avrdude.conf"
    ],
    "macOS": {
      "entitlements": null,
      "minimumSystemVersion": "10.15"
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
